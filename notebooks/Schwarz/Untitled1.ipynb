   "source": [
    "M=[[0,1],[2,0]]\n",
    "\n",
    "def mausi(M):\n",
    "    max_degree = max(_[-1] for _ in M)\n",
    "    pupsi = [_ for _ in M if _[-1] == max_degree]\n",
    "    for i in reversed(range(0, len(M[0]) - 1)):\n",
    "        print (i)\n",
    "    print(pupsi)\n"
   ]
